<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.ecatalogue">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        package="com.sit.gems.activity"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactUsActivityy"></activity>
        <activity
            android:name=".Activity.TermsConditionActivity"
            />
        <activity
            android:name=".Activity.CultureActivity"
          />
        <activity
            android:name=".Activity.AboutUsActivity"
             />
        <activity
            android:name=".CultureActivity"
           />
        <activity android:name=".Activity.ItemDetailsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e.ecatalogue.Activity.ItemsActivity" />
        </activity>
        <activity android:name=".Activity.ItemsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e.ecatalogue.Activity.CategoriesActivity" />

        </activity>
        <activity android:name=".Activity.CategoriesActivity"
          >

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e.ecatalogue.Activity.BrandsActivity" />
        </activity>
        <activity
            android:name=".Activity.BrandsActivity"
            android:label="@string/app_name">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e.ecatalogue.Activity.LoginActivity" />
        </activity>
        <activity android:name=".Activity.RegisterActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e.ecatalogue.MainActivity" />
        </activity>
        <activity android:name=".Activity.LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.e.ecatalogue.MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize|mcc|mnc">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>